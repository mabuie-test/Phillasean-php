{
  "name": "phil-asean/provider-logistics",
  "description": "Backend PHP + MySQL para PHIL ASEAN Provider & Logistics",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "phpmailer/phpmailer": "^6.8",
    "setasign/fpdf": "^1.8",
    "tecnickcom/tcpdf": "^6.6",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "App\\Config\\": "config/",
      "App\\Auth\\": "auth/",
      "App\\User\\": "user/",
      "App\\Admin\\": "admin/",
      "App\\Mailer\\": "mailer/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "echo \"Dependências instaladas com sucesso!\""
    ],
    "post-update-cmd": [
      "echo \"Dependências atualizadas com sucesso!\""
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  }
}
